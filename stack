#include <iostream>
#define SIZE 10
using namespace std;
class Statck
{
int data[SIZE];
int top;
public:
Statck()
{
top= -1;
}
void push(int element)
{
    if(isFull() ==1 )
     cout<<"stack is full";
     else
    data[++top]=element;
}
int pop()
{
    if(isEmpty()== 1)
    return -1;
    else 
    return data[top--];
}
int isEmpty()
{
    if(top== -1)
    return 1;
    else 
    return 0;
}
int isFull()
{
    if(top == SIZE -1 )
    return 1;
    else
    return 0;
}
int peek()
{
    if(isEmpty()==1)
    return -1;
    else
    return data[top];
}
};
int main()
{
    cout<<"static implementation of stack";
    Statck st;
    st.push(10);
    st.push(20);
    cout<<"\n stack is empty"<< st.isEmpty();
    cout<<"\n stack is full"<<    st.isFull();
    cout<<"\n element at top position"<< st.peek();
    cout<<"\n element is removed"<< st.pop();
     cout<<"\n element is removed"<< st.pop();
      cout<<"\n element is removed"<< st.pop();
    return 0;
}
